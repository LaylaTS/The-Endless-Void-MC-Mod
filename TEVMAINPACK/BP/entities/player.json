{
	"format_version": "1.18.20",
	"minecraft:entity": {
		"description": {
			"identifier": "minecraft:player",
			"is_spawnable": false,
			"is_summonable": false,
			"is_experimental": false
		},
		"component_groups": {
			"tev:mageclass": {
				"minecraft:health": {
					"value": 20,
					"max": 20
				},
				"minecraft:attack": {
					"damage": 1
				},
				"minecraft:movement": {
					"value": 0.1
				},
				"minecraft:knockback_resistance": {
					"value": 0
				}
			},
			"tev:tankclass": {
				"minecraft:health": {
					"max": 40,
					"value": 40
				},
				"minecraft:attack": {
					"damage": 1
				},
				"minecraft:movement": {
					"value": 0.8
				},
				"minecraft:knockback_resistance": {
					"value": 0.5
				}
			},
			"tev:normalmode": {
				"minecraft:damage_sensor": {
					"triggers": {
						"cause": "all",
						"on_damage": {
							"event": "tev:on_damageevents",
							"target": "self"
						}
					}
				}
			},
			"tev:apocalypsemode": {
				"minecraft:damage_sensor": {
					"triggers": {
						"cause": "all",
						"damage_multiplier": 2,
						"on_damage": {
							"event": "tev:on_damageevents",
							"target": "self"
						}
					}
				}
			},
			"tev:deathmode": {
				"minecraft:damage_sensor": {
					"triggers": {
						"cause": "all",
						"damage_multiplier": 10000000,
						"on_damage": {
							"event": "tev:on_damageevents"
						}
					}
				}
			},
			"tev:assassinclass": {
				"minecraft:health": {
					"max": 16,
					"value": 16
				},
				"minecraft:attack": {
					"damage": 8,
					"effect_duration": 2,
					"effect_name": "slowness"
				},
				"minecraft:movement": {
					"value": 0.13
				},
				"minecraft:knockback_resistance": {
					"value": 0
				}
			}
		},
		"components": {
			"minecraft:on_death": {
				"event": "tev:ondeathevent"
			},
			"minecraft:experience_reward": {
				"on_death": "math.min(query.player_level*7,100)"
			},
			"minecraft:type_family": {
				"family": [
					"player"
				]
			},
			"minecraft:is_hidden_when_invisible": {},
			"minecraft:loot": {
				"table": "loot_tables/empty.json"
			},
			"minecraft:collision_box": {
				"width": 0.6,
				"height": 1.8
			},
			"minecraft:can_climb": {},
			"minecraft:movement": {
				"value": 0.11
			},
			"minecraft:hurt_on_condition": {
				"damage_conditions": [
					{
						"filters": {
							"test": "in_lava",
							"subject": "self",
							"operator": "==",
							"value": true
						},
						"cause": "lava",
						"damage_per_tick": 6
					}
				]
			},
			"minecraft:attack": {
				"damage": 1
			},
			"minecraft:player.saturation": {
				"value": 20
			},
			"minecraft:player.exhaustion": {
				"value": 0,
				"max": 4
			},
			"minecraft:player.level": {
				"value": 0,
				"max": 500
			},
			"minecraft:player.experience": {
				"value": 0,
				"max": 1
			},
			"minecraft:on_hurt": {
				"event": "tev:on_damageevents",
				"target": "self"
			},
			"minecraft:breathable": {
				"total_supply": 15,
				"suffocate_time": -1,
				"inhale_time": 3.75,
				"generates_bubbles": false
			},
			"minecraft:nameable": {
				"always_show": true,
				"allow_name_tag_renaming": false
			},
			"minecraft:physics": {},
			"minecraft:pushable": {
				"is_pushable": false,
				"is_pushable_by_piston": true
			},
			"minecraft:insomnia": {
				"days_until_insomnia": 3
			},
			"minecraft:rideable": {
				"seat_count": 2,
				"family_types": [
					"parrot_tame"
				],
				"pull_in_entities": true,
				"seats": [
					{
						"position": [
							0.4,
							-0.2,
							-0.1
						],
						"min_rider_count": 0,
						"max_rider_count": 0,
						"lock_rider_rotation": 0
					},
					{
						"position": [
							-0.4,
							-0.2,
							-0.1
						],
						"min_rider_count": 1,
						"max_rider_count": 2,
						"lock_rider_rotation": 0
					}
				]
			},
			"minecraft:conditional_bandwidth_optimization": {},
			"minecraft:block_climber": {}
		},
		"events": {
			"tev:normalmode": {
				"remove": {
					"component_groups": [
						"tev:apocalypsemode",
						"tev:deathmode"
					]
				},
				"add": {
					"component_groups": [
						"tev:normalmode"
					]
				},
				"run_command": {
					"command": "/scoreboard players set @s difficulty 0"
				}
			},
			"tev:apocalypsemode": {
				"remove": {
					"component_groups": [
						"tev:normalmode",
						"tev:deathmode"
					]
				},
				"add": {
					"component_groups": [
						"tev:apocalypsemode"
					]
				},
				"run_command": {
					"command": "/scoreboard players set @s difficulty 1"
				}
			},
			"tev:deathmode": {
				"remove": {
					"component_groups": [
						"tev:normalmode",
						"tev:deathmode"
					]
				},
				"add": {
					"component_groups": [
						"tev:deathmode"
					]
				},
				"run_command": {
					"command": "/scoreboard players set @s difficulty 2"
				}
			},
			"tev:on_damageevents": {
				"sequence": [
					{
						/*THORNS ACCESSORY*/
						"run_command": {
							"command": [
								"/damage @e[type=!player,type=!npc,r=4] 1 thorns"
							],
							"target": "self"
						},
						"filters": {
							"test": "has_equipment",
							"subject": "self",
							"value": "tev:thornsaccessory",
							"domain": "hand"
						}
					}
				]
			},
			"tev:ondeathevent": {
				"sequence": [
					{
						/* Forbidden Plant Ability*/
						"sequence": [
							{
								"filters": {
									"test": "has_equipment",
									"subject": "self",
									"value": "tev:forbiddenplant",
									"domain": "hand"
								},
								"run_command": {
									"command": "/effect @a[r=8] instant_health 0 1"
								}
							}
						]
					},
					{
						/* FOG RESET */
						"run_command": {
							"command": [
								"/fog @s push minecraft:fog_default mainfog",
								"/fog @s remove mainfog",
								"/fog @s remove mainfog"
							]
						}
					}
				]
			},
			"tev:magetoggle": {
				"sequence": [
					{
						"remove": {
							"component_groups": [
								"tev:tankclass",
								"tev:assassinclass"
							]
						}
					},
					{
						"add": {
							"component_groups": [
								"tev:mageclass"
							]
						}
					},
					{
						"run_command": {
							"command": "/scoreboard players set @s class 2",
							"target": "self"
						}
					}
				]
			},
			"tev:assassintoggle": {
				"sequence": [
					{
						"remove": {
							"component_groups": [
								"tev:mageclass",
								"tev:tankclass"
							]
						}
					},
					{
						"add": {
							"component_groups": [
								"tev:assassinclass"
							]
						}
					},
					{
						"run_command": {
							"command": "/scoreboard players set @s class 1",
							"target": "self"
						}
					}
				]
			},
			"tev:tanktoggle": {
				"sequence": [
					{
						"remove": {
							"component_groups": [
								"tev:mageclass",
								"tev:assassinclass"
							]
						}
					},
					{
						"add": {
							"component_groups": [
								"tev:tankclass"
							]
						}
					},
					{
						"run_command": {
							"command": "/scoreboard players set @s class 3",
							"target": "self"
						}
					}
				]
			}
		}
	}
}